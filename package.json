    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "electron": "~9.4.0",
    "electron-builder": "^20.39.0",
    "electron-icon-maker": "0.0.4",
    "electron-react-devtools": "^0.5.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
